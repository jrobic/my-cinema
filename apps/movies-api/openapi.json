{
  "openapi": "3.0.3",
  "info": {
    "title": "movies-api",
    "description": "",
    "license": {
      "name": ""
    },
    "version": "0.1.0"
  },
  "paths": {
    "/health": {
      "get": {
        "tags": [
          "Core"
        ],
        "operationId": "health",
        "responses": {
          "200": {
            "description": "Return Ok when api is available",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/me": {
      "get": {
        "tags": [
          "Me"
        ],
        "operationId": "me_ctrl",
        "responses": {
          "200": {
            "description": "Return user information",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "required": [
          "firstName",
          "lastName",
          "age"
        ],
        "properties": {
          "age": {
            "type": "integer",
            "format": "int32",
            "minimum": 0.0
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          }
        }
      }
    }
  },
  "security": []
}